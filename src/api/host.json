{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "watchFiles": [ "events.db" ],
  "extensions": {
    "http": {
      "routePrefix": "",
      "customHeaders": {
        "X-Content-Type-Options": "nosniff",
        "Content-Type": "application/json; charset=utf-8"
      }
    }
  }
}
